<%- include('includes/head.ejs')%>
    </head>
    <body id="page-top">
        <!-- Navigation-->
        <%- include('includes/nav.ejs')%>
        <!-- Header-->
        <header class="masthead text-center text-white">
            <div class="masthead-content">
                <div class="container px-1">
                    <h2 class="masthead-subheading mb-0">รหัสวิชา <%= coursedt.course_id %></h2>
                </div>
            </div>
        </header>
        <!-- Content section 1-->
        <section id="">
          
            
            <h5><i class="fa-solid fa-desktop"></i> รหัสวิชา <%= coursedt.course_id %></h5>
            <h2><%= coursedt.course_name %></h2>
            <h4><%= coursedt.course_name_eng %></h4>
            <ul>
                <li class="description_study">
                    <strong>หน่วยกิต (บรรยาย-ปฎิบัติการ): </strong><%= coursedt.credit %>
                </li>
                    <% if (coursedt.pre_requisite !=  "") { %>
                        <li class="description_study">
                            <strong><%= coursedt.pre_requisite %></strong>
                        </li>
                    <% } %>
                        <li><strong>เนื้อหา: </strong><%= coursedt.course_description %></li>
                        <%= coursedt.course_description_eng %>
            </ul>
        </section>
        
        <!-- Footer-->
        <%- include('includes/footer.ejs')%>
        <!-- Bootstrap core JS-->
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"></script>
        <!-- Core theme JS-->
        <script src="js/scripts.js"></script>
    </body>
</html>
